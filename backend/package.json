{
  "name": "auth-template",
  "version": "1.0.0",
  "description": "auth template",
  "main": "build/server.js",
  "scripts": {
    "start": "nodemon src/server.ts",
    "build": "tsc -p .",
    "typeorm": "typeorm-ts-node-esm -d ./src/config/data-source//then you can do: npm run typeorm migration:create ./path-to-migration//use this to replace below scripts//typeorm = -d ./data-source",
    "migration:create": "typeorm migration:create ./src/migrations/NoteRefactoring",
    "migration:generate": "typeorm-ts-node-esm migration:generate ./src/migrations/NoteRefactoring -d ./src/config/data-source",
    "migration:run": "typeorm-ts-node-esm migration:run -d ./src/config/data-source",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d ./src/config/data-source",
    "lint": "eslint . --ext .ts",
    "eslint:fix": "eslint --fix",
    "client": "npm run dev --prefix ../frontend",
    "dev": "concurrently \" npm run client\" \" npm start\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mongoose-sequence": "^3.0.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-github2": "^1.2.5",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-linkedin-oauth2": "^1.5.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@superfaceai/passport-twitter-oauth2": "^1.2.3",
